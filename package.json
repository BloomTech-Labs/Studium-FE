{
  "name": "synaps",
  "version": "0.1.0",
  "engines": {
    "node": "13.x.x"
  },
  "private": true,
  "dependencies": {
    "antd": "^3.26.9",
    "axios": "^0.19.2",
    "firebase": "^7.14.0",
    "moment": "^2.24.0",
    "polished": "^3.5.1",
    "pretty-format": "^25.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "latest",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.0.1",
    "styled-reset": "^4.1.0",
    "styled-theming": "^2.2.0",
    "typeface-source-sans-pro": "^1.1.5"
  },
  "scripts": {
    "start": "react-scripts start ",
    "build": "react-scripts build ",
    "test": "react-scripts test ",
    "eject": "react-scripts eject",
    "clear": "react-scripts test --clearCache",
    "coverage": "react-scripts test --coverage --watchAll=false || true",
    "clear:run": "CI=true npm-run-all clear coverage || true",
    "lint:css": "stylelint ./src/**/*.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/config/firebase/**",
      "!*.test.js",
      "!*.spec.js",
      "!**/docs/**",
      "!*.md"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid",
    "useTabs": false,
    "printWidth": 80,
    "jsxBracketSameLine": false
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "latest",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^25.1.3",
    "@types/prop-types": "^15.7.3",
    "@types/react-redux": "^7.1.7",
    "@types/testing-library__jest-dom": "^5.0.1",
    "@types/testing-library__react": "^9.1.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "jest-styled-components": "^7.0.0",
    "moxios": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.5.0",
    "postcss-styled": "^0.34.0",
    "prettier": "^1.19.1",
    "react-styleguide-generator": "^1.4.0",
    "react-styleguidist": "^10.6.2",
    "react-test-renderer": "^16.13.0",
    "redux-devtools-extension": "^2.13.8",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "supertest": "^4.0.2"
  }
}
